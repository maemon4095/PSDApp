{
    "deno.enable": true,
    "deno.lint": true,
    "deno.config": "./deno.json",
    "deno.disablePaths": [
        "./dist"
    ],
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": "on"
    },
    "extensions.ignoreRecommendations": false,
    "svg.preview.background": "transparent"
}