{
  "compilerOptions": {
    "lib": ["deno.window", "dom", "dom.iterable"],
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "noImplicitOverride": true
  },
  "imports": {
    "~/": "./src/",
    "preact": "https://esm.sh/preact@10.19.7",
    "preact/hooks": "https://esm.sh/preact@10.19.7/hooks",
    "@types/file.d.ts": "../../plugins/loaderOverride/types/file.d.ts"
  },
  "tasks": {
    "dev": "deno run -A build.ts serve",
    "build": "deno run -A build.ts build",
    "gen": "deno run -A build.ts gen",
    "preview": "deno run -A ./preview/build.ts"
  }
}
